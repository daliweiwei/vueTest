.el-radio-button__orig-radio
  &:checked
    +.el-radio-button__inner
      background-color: #52c41a;
      border-color: #52c41a;
      box-shadow: -1px 0 0 0 #52c41a;

.el-tabs__nav-wrap
  background #fff
.el-tabs__active-bar
  background #52c41a
.el-tabs__item
  height 50px
  line-height 50px
  color #595959
  &:hover
    color #52c41a
  &.is-active
    border-bottom 3px solid #52c41a
    box-sizing border-box
    color #52c41a
